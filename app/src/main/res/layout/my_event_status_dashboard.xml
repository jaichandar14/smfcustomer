<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="events_dashboard_view_model"
            type="com.smf.customer.view.dashboard.fragment.serviceFragment.eventListDashBoard.EventsDashBoardViewModel" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/costraint_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">


        <TextView
            android:id="@+id/dashboard_tx"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_12sdp"
            android:fontFamily="@font/mulish_bold"
            android:gravity="top"
            android:text="@string/dashboard"
            android:textColor="@color/black"
            android:textSize="@dimen/_14sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:background="@drawable/theme_corner_radius_white_background"
            android:backgroundTint="@color/fade_gray"
            android:padding="@dimen/_15sdp"
            android:textColor="@color/gray_text"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dashboard_tx"
            app:layout_constraintVertical_bias="0.0">

            <TextView
                android:id="@+id/title_txt"
                android:layout_width="@dimen/_120sdp"
                android:maxLength="15"
                android:maxLines="1"
                android:layout_height="wrap_content"
                android:fontFamily="@font/mulish_bold"
                android:text="@string/saraas_birthday"
                android:textColor="@color/black_event_title"
                android:textSize="@dimen/_14ssp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />


            <TextView
                android:id="@+id/event_date"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_15sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:fontFamily="@font/mulish_bold"
                android:gravity="center_vertical"
                android:text="@string/event_dates"
                android:textColor="@color/black_event_title"
                android:textSize="@dimen/_10ssp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/title_txt"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/event_date_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_2sdp"
                android:fontFamily="@font/mulish_bold"
                android:text="@string/_12_aug_2021"
                android:textColor="@color/black_event_title"
                android:textSize="@dimen/_11ssp"
                app:layout_constraintBottom_toBottomOf="@+id/event_date"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/event_date"
                app:layout_constraintTop_toTopOf="@+id/event_date"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:layout_marginTop="@dimen/_5sdp"
                android:id="@+id/event_view_details"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/mulish_bold"
                android:gravity="center_vertical"
                android:text="@string/view_event_details"
                android:textColor="@color/darkblue_smf_font"
                android:textSize="@dimen/_10ssp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event_date"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/my_event_icon"
                android:layout_width="@dimen/_28sdp"
                android:layout_height="@dimen/_28sdp"
                android:contentDescription="@string/icon"
                android:src="@drawable/availability"
                app:layout_constraintBottom_toBottomOf="@+id/title_txt"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.95"
                app:layout_constraintStart_toEndOf="@+id/title_txt"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

            <HorizontalScrollView
                android:id="@+id/horizontal_scroll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_12sdp"
                android:scrollbars="none"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/event_view_details"
                app:layout_constraintVertical_bias="0.0">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/add_service_icon"
                        android:layout_width="@dimen/_35sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginStart="@dimen/_20sdp"
                        android:contentDescription="@string/add_or_remove_services"
                        android:src="@drawable/new_selection"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_5sdp"
                        android:contentDescription="@string/dotted_line"
                        android:layerType="software"
                        android:src="@drawable/dotted_line"
                        app:layout_constraintBottom_toBottomOf="@+id/order_details_icon"
                        app:layout_constraintEnd_toStartOf="@+id/order_details_icon"
                        app:layout_constraintStart_toEndOf="@+id/add_service_icon"
                        app:layout_constraintTop_toTopOf="@+id/add_service_icon" />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guideline4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        app:layout_constraintGuide_begin="@dimen/_40sdp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/mulish_medium"
                        android:text="@string/add_or_remove_services"
                        android:textAlignment="center"
                        android:maxEms="6"
                        android:maxLines="2"
                        android:textColor="@color/black_event_title"
                        android:textSize="@dimen/_10ssp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/add_service_icon"
                        app:layout_constraintHorizontal_bias="0.50"
                        app:layout_constraintStart_toStartOf="@+id/add_service_icon"
                        app:layout_constraintTop_toBottomOf="@+id/guideline4"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageView
                        android:id="@+id/order_details_icon"
                        android:layout_width="@dimen/_35sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginStart="@dimen/_67sdp"
                        android:contentDescription="@string/add_or_remove_services"
                        android:src="@drawable/new_selection"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/add_service_icon"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_5sdp"
                        android:contentDescription="@string/dotted_line"
                        android:layerType="software"
                        android:src="@drawable/dotted_line"
                        app:layout_constraintBottom_toBottomOf="@+id/admin_approval"
                        app:layout_constraintEnd_toStartOf="@+id/admin_approval"
                        app:layout_constraintStart_toEndOf="@+id/order_details_icon"
                        app:layout_constraintTop_toTopOf="@+id/order_details_icon" />

                    <TextView
                        android:layout_width="@dimen/_65sdp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/mulish_medium"
                        android:text="@string/order_details"
                        android:textAlignment="center"
                        android:textColor="@color/black_event_title"
                        android:textSize="@dimen/_10ssp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/order_details_icon"
                        app:layout_constraintStart_toStartOf="@+id/order_details_icon"
                        app:layout_constraintTop_toBottomOf="@+id/guideline4"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageView
                        android:id="@+id/admin_approval"
                        android:layout_width="@dimen/_35sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginStart="@dimen/_67sdp"
                        android:contentDescription="@string/add_or_remove_services"
                        android:src="@drawable/new_selection"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/order_details_icon"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_5sdp"
                        android:contentDescription="@string/dotted_line"
                        android:layerType="software"
                        android:src="@drawable/dotted_line"
                        app:layout_constraintBottom_toBottomOf="@+id/bidding_status_icon"
                        app:layout_constraintEnd_toStartOf="@+id/bidding_status_icon"
                        app:layout_constraintStart_toEndOf="@+id/admin_approval"
                        app:layout_constraintTop_toTopOf="@+id/admin_approval" />

                    <TextView
                        android:layout_width="@dimen/_65sdp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/mulish_medium"
                        android:text="@string/admin_approval"
                        android:textAlignment="center"
                        android:textColor="@color/black_event_title"
                        android:textSize="@dimen/_10ssp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/admin_approval"
                        app:layout_constraintStart_toStartOf="@+id/admin_approval"
                        app:layout_constraintTop_toBottomOf="@+id/guideline4"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageView
                        android:id="@+id/bidding_status_icon"
                        android:layout_width="@dimen/_35sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginStart="@dimen/_67sdp"
                        android:contentDescription="@string/add_or_remove_services"
                        android:src="@drawable/new_selection"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/admin_approval"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_5sdp"
                        android:contentDescription="@string/dotted_line"
                        android:layerType="software"
                        android:src="@drawable/dotted_line"
                        app:layout_constraintBottom_toBottomOf="@+id/payment"
                        app:layout_constraintEnd_toStartOf="@+id/payment"
                        app:layout_constraintStart_toEndOf="@+id/bidding_status_icon"
                        app:layout_constraintTop_toTopOf="@+id/payment" />

                    <TextView
                        android:layout_width="@dimen/_65sdp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="6dp"
                        android:fontFamily="@font/mulish_medium"
                        android:text="@string/bidding_status"
                        android:textAlignment="center"
                        android:textColor="@color/black_event_title"
                        android:textSize="@dimen/_10ssp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/bidding_status_icon"
                        app:layout_constraintStart_toStartOf="@+id/bidding_status_icon"
                        app:layout_constraintTop_toBottomOf="@+id/guideline4"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageView
                        android:id="@+id/payment"
                        android:layout_width="@dimen/_35sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginStart="@dimen/_67sdp"
                        android:contentDescription="@string/add_or_remove_services"
                        android:src="@drawable/new_selection"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/bidding_status_icon"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_5sdp"
                        android:contentDescription="@string/dotted_line"
                        android:layerType="software"
                        android:src="@drawable/dotted_line"
                        app:layout_constraintBottom_toBottomOf="@+id/feedback"
                        app:layout_constraintEnd_toStartOf="@+id/feedback"
                        app:layout_constraintStart_toEndOf="@+id/payment"
                        app:layout_constraintTop_toTopOf="@+id/payment" />


                    <TextView
                        android:layout_width="@dimen/_65sdp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/mulish_medium"
                        android:text="@string/payment"
                        android:textAlignment="center"
                        android:textColor="@color/black_event_title"
                        android:textSize="@dimen/_10ssp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/payment"
                        app:layout_constraintStart_toStartOf="@+id/payment"
                        app:layout_constraintTop_toBottomOf="@+id/guideline4"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageView
                        android:id="@+id/feedback"
                        android:layout_width="@dimen/_35sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_marginStart="@dimen/_67sdp"
                        android:contentDescription="@string/add_or_remove_services"
                        android:src="@drawable/new_selection"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/payment"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />

                    <TextView
                        android:layout_width="@dimen/_65sdp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/mulish_medium"
                        android:text="@string/feedback"
                        android:textAlignment="center"
                        android:textColor="@color/black_event_title"
                        android:textSize="@dimen/_10ssp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/feedback"
                        app:layout_constraintStart_toStartOf="@+id/feedback"
                        app:layout_constraintTop_toBottomOf="@+id/guideline4"
                        app:layout_constraintVertical_bias="0.0" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </HorizontalScrollView>
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>

